    <script type="text/javascript">
        var handlerAddr = "http://qrcode.tof.oa.com";
        // var handlerAddr = "http://localhost:29584"
        var queryString = "?url=http%3A%2F%2Fiwiki.oa.com%2F_sp_login_%2F%3Furl%3D%252Fdownload%252Fattachments%252F17700398%252Fimage2019-10-11_23-5-3.png%253Fversion%253D1%2526modificationDate%253D1570806300000%2526api%253Dv2%26app%3D";

        // Login logic variables
        var webPollInterval = 1000;
        var isShowingSignInBoard;
        var username;
        var loginSource;
        var pollTimeOut;
        var showUserInfoTimeOut;

        // For Displaying ADs in turn
        var bannerIdPrefix = "top_banner_";
        var ad_num = 0;
        var ad_index = 0;

        $(document).ready(initialize);

        function GetAdsenseImagesByHandler() {
            $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                url: "/Modules/Passport/AdsenseHandler.ashx",
                data: "{}",
                dataType: 'json',
                success: function (result) {
                    if (result != null) {
                        onAjaxImage(result);
                    }

                }
            });
        }

      
           
        function onAjaxImage(data) {
            var list = data;
            if (list != null) {
                for (var i = 0; i < list.length; i++) {
                    switch (list[i].TypeId) {
                        case 0:
                        default:
                            SetAdsenseImg(list[i]);
                            break;
                    }
                }
            }
        }

        function SetAdsenseImg(data) {
            var result = data;
            if (result != null) {
                var htmldoc = getBannerHtml(result);
                if(ad_num == 0) {
                    $(".banner_wrap").replaceWith(htmldoc);
                } else {
                    $(".banner_wrap").append(htmldoc);
                }
                ad_num ++;
            }
        }
        
        function getBannerHtml(result) {
            var imglink = "#";
            if (result.ImgLink != "") {
                imglink = result.ImgLink;
            }
            var imgSrc = "http://tof2.oa.com" + result.ImgName;

            // Need setting background
            var htmldoc = "<div class='rtx_top' id = '" + bannerIdPrefix + ad_num + "' style = 'background-color:#" + result.BgColor + "'>";
            htmldoc += "<div class='rtx_top_banner'>";
            htmldoc += "<a href='" + imglink + "' target='_blank'>";
            htmldoc += "<img alt='" + result.Title + "' src='" + imgSrc + "' />";
            htmldoc += "</a></div></div>";
            return htmldoc;
        }

        // Top banner
        function showTopBanner() {
            if (ad_num < 2) {
                $('.rtx_top_banner').css("display", "block");
            } else {
                showAd();
                setInterval(showAd, 0);
            }
        }

        // display ADs in turn
        // If there is more than 1 advertisement, the second one will be displayed first
        function showAd() {
            $(bannerIdPrefix + ad_index).fadeOut();
            ad_index = (ad_index + 1) % add_num;
            $(bannerIdPrefix + ad_index).fadeIn();
        }


        // show the info of current login user
        function showUserInfo() {
            if (isShowingSignInBoard) {
                $(".login_wrap").addClass("login_authorize_show");
                $("#user_name").text(username);
                var user_image_src ="/Modules/Passport/UserImage.ashx?user=" + username;
                $("#user_image").attr("src", user_image_src);
                var srcText = "请在MOA确认登录<br/>Confirm in MOA"
                $("#icon_scansrc").attr("class", "icon_phone");
                if(!loginSource){
                    loginSource = "MOA";
                    $("#icon_scansrc").attr("class", "icon_phone");
                    srcText = "请在" + loginSource + "确认登录<br/>Confirm in MOA"
                }else if(loginSource == "企业微信"){
                    $("#icon_scansrc").attr("class", "icon_qywx");
                    srcText = "请在" + loginSource + "确认登录<br/>Confirm in WeChat Work"
                }
                $("#user_confirmsource").html(srcText);
                isShowingSignInBoard = false;
            }
        }

        // return the login page
        function showSignInBoard() {
            if (!isShowingSignInBoard) {
                $(".login_qr_code").removeClass("scan_success_show");
                $(".login_wrap").removeClass("login_authorize_show");
                isShowingSignInBoard = true;
            }
        }

        function clientquickclick(){
            var ioausername = $("label[name='ioausername']").text();
            $("input[name='txtLoginName']").text(ioausername);
            $("input[name='txtPassword']").text(ioausername);
            return true;
        }
    </script>
